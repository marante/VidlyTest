@model List<VidlyUdemyTest.Models.Movie>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>

<table class="table table-striped table-bordered">
    <thead>
    <tr>
        <th>Customer</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var movies in Model)
    {
        <tr>
            <td>
                @movies.Name
            </td>
        </tr>
    }
    </tbody>
</table>
